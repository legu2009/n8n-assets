{
  "name": "local_n8n_static",
  "version": "1.0.1",
  "description": "n8n静态文件资源",
  "scripts": {
    "publish": "npm publish --registry=https://registry.npmjs.org",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "hono": "^4.8.2",
    "@hono/node-server": "^1.14.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,jsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,css,less,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "n8n-assets",
    "n8n-chinese-assets"
  ],
  "author": "legu2009"
}